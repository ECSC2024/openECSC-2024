{
  "enabled": true,
  "services": {
    "web": {
      "build": "src",
      "environment": {
        "DB_HOST": "{{ .services.db.hostname }}",
        "DB_USER": "sharepic",
        "DB_PASS": "98a4f7383513e12065ffd632",
        "DB_NAME": "sharepic"
      },
      "volumes": [
        {
          "type": "bind",
          "readonly": true,
          "host": "src/init.sql",
          "container": "/docker-entrypoint-initdb.d/init.sql"
        }
      ],
      "expose": [
        {
          "type": "tcp",
          "container": 80,
          "host": 38211
