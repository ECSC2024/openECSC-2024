{
  "enabled": true,
  "services": {
    "asmprovider": {
      "image": "cybersecnatlab/socaz:ubuntu-jammy",
      "command": [
        "socaz",
        "--timeout",
        "120",
        "--bind",
        "31337",
        "--cmd",
        "/home/user/chall"
      ],
      "volumes": [
        {
          "type": "bind",
          "readonly": true,
          "host": "src/asm_provider/provider",
          "container": "/home/user/chall"
        }
      ],
      "expose": [
        {
          "type": "tcp",
          "container": 31337,
          "host": 38205
        }
      ]
    },
    "licensechecker": {
      "image": "cybersecnatlab/challenge-jail@sha256:8ac55aa6e7fe753a43b9b5a0283113544742798aaef7977eab0e854c476a634a",
      "environment": {
        "FLAG": "CCIT{m4yb3_runn1ng_th3_v4lid4t10n_c0d3_0n_my_cl13n7s_l4pt0ps_1sn't_such_4_gr3a7_1dea_04b5e95c}",
        "FLAG_IN_ENV": "true",
        "FORWARDED_ENV": "REMOTE,PORT",
        "REMOTE": "{{ .services.asmprovider.hostname }}",
        "PORT": "31337",
        "TIMEOUT": "120",
        "NETWORK": "true"
      },
      "volumes": [
        {
          "type": "bind",
          "readonly": true,
          "host": "src/chall/chall",
          "container": "/home/user/chall"
        }
      ],
      "expose": [
        {
          "type": "tcp",
          "container": 1337,
          "host": 38204
        }
      ],
      "depends-on": [
        "asmprovider"
      ]
    }
  }
}
